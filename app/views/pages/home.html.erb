<div class="fullpage">
	<div class="section one" data-controller="typed">
    <div class="header-container">
      <h1 class="section-header header-upper">HELLO</h1>
      <hr>
    </div>
    <div id="typed-strings" class="section-content">
      <p><span class="typed"> </span></p>
    </div>
    <script>
        var typed = new Typed(".typed",{
        strings: [,  "Welcome to my profile","Nice to meet you, my name is Jess","I'm a FS Web Developer in training","Scroll down to find out more"],
        typeSpeed: 40,
        backSpeed: 10,
        loop: true
        })
    </script>
  </div>

	<div class="section two">
    <div class="section-content">
    </div>
    <div class="header-container">
      <h1 class="section-header header-lower"><a id="about-me">ABOUT ME</a></h1>
